#include<iostream>
#include<string.h>
using namespace std;
int main()
{
   long long int g=0,s=0,i,l;
    char str[100000];
    cin>>str;
    l=strlen(str);
    if(str[l-1]==54)
    cout<<"-1"<<endl;
    else{
    for(i=0;i<l;i++)
    {
        if(str[i]==54)
        s++;
      
        else
          g++;
    }

      cout<<g;
    }
}
